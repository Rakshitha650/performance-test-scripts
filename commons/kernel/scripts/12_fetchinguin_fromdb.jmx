            <stringProp name="script">import java.io.*;

int uin_count = Integer.parseInt(vars.get(&quot;uin_#&quot;));
System.out.println(uin_count);
String basepath = vars.get(&quot;data_path&quot;);
File file = new File(basepath+&quot;\\uin_list_from_db.txt&quot;);
System.out.println(file.getAbsolutePath());
FileWriter fw = new FileWriter(file.getAbsolutePath(),true);
BufferedWriter bw = new BufferedWriter(fw);
for(int i=1; i&lt;=uin_count;i++){
	String uin = vars.get(&quot;uin_&quot;+i);
	bw.write(uin+&quot;\r\n&quot;);
	}

bw.close();</stringProp>
